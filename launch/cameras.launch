<?xml version="1.0" ?>
<launch>
	<node name="camera1_node" pkg="cv_camera" type="cv_camera_node" ns="camera1">
		<rosparam command="load" file="$(find asv_sensors)/config/cameras.config" />
		<param name="device_id" value="0" />
		<param name="frame_id" value="camera1" />
	</node>

	<node name="camera1_node" pkg="cv_camera" type="cv_camera_node" ns="camera2">
		<rosparam command="load" file="$(find asv_sensors)/config/cameras.config" />
		<param name="device_id" value="1" />
		<param name="frame_id" value="camera2" />
	</node>

	<node name="camera1_node" pkg="cv_camera" type="cv_camera_node" ns="camera3">
		<rosparam command="load" file="$(find asv_sensors)/config/cameras.config" />
		<param name="device_id" value="2" />
		<param name="frame_id" value="camera3" />
	</node>
</launch>