<?xml version="1.0"?>
<launch>


	<arg name="gps_com" default="/dev/ttyUSB0" />
	<arg name="imu_com" default="/dev/ttyUSB1" />

	<include file="$(find asv_sensors)/launch/ekf_prep.launch">
		<arg name="gps_com" value="$(arg gps_com)" />
		<arg name="imu_com" value="$(arg imu_com)" />
	</include>

	<node pkg="robot_pose_ekf" type="robot_pose_ekf" name="robot_pose_ekf">
		<param name="output_frame" value="odom" />
		<param name="freq" value="100" />
		<param name="odom_used" value="true" />
		<param name="imu_used" value="true" />
		<param name="vo_used" value="false" />
		<param name="gps_used" value="false" />
	</node>

</launch>
