<?xml version="1.0" ?>
<launch>

	<arg name="imu_port" default="/dev/ttyUSB0" />
	<arg name="gps_port" default="/dev/ttyUSB1" />
	<arg name="gps_rfd" default="/dev/ttyUSB2" />
	<arg name="lidar_host" default="192.168.1.100" />

	<node name="gps_comm" pkg="nmea_navsat_driver" type="nmea_serial_driver">
		<param name="port" value="$(arg gps_port)" />
		<param name="baud" value="38400" />
		<param name="frame_id" value="gps" />
		<!--<remap from="/fix" to="/boat_fix" />-->
	</node>

	<!--<node name="base_gps_comm" pkg="nmea_navsat_driver" type="nmea_topic_serial_reader">
		<param name="port" value="$(arg gps_rfd)" />
		<param name="baud" value="57600" />
		<param name="frame_id" value="base_gps" />
		<remap from="/nmea_sentence" to="/base_nmea_sentence" />
	</node>

	<node name="base_gps_parse" pkg="gps_parser" type="parser">
		<remap from="/nmea_sentence" to="/base_nmea_sentence" />
		<remap from="/fix" to="/base_fix" />
		<remap from="/vel" to="/base_vel" />
	</node>-->

	
	<include file="$(find asv_sensors)/launch/imu.launch">
		<arg name="imu_port" value="$(arg imu_port)" />
	</include>

	
	<node name="lms1xx" pkg="lms1xx" type="LMS1xx_node">
		<param name="host" value="$(arg lidar_host)" />
		<param name="frame_id" value="base_link" />
	</node>

</launch>